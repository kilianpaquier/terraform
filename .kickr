# Kickr configuration file (https://github.com/kickr-dev/kickr)
# yaml-language-server: $schema=https://raw.githubusercontent.com/kickr-dev/kickr/beta/.schemas/kickr.v1.schema.json
version: 1
ci:
  provider: gitlab
  options:
    - kickr
  terraform:
    apply: manual
    environments:
      - production
dependencies:
  manager: renovate
exclude:
  - shell
license: mit
maintainers:
  - name: kilianpaquier
platform: gitlab
terraform:
  engine: opentofu
  modules:
    - modules/git
